{
  "name": "describe-it",
  "version": "1.7.0",
  "description": "Extracts a private function / variable for BDD unit testing",
  "main": "describe-it.js",
  "scripts": {
    "test": "mocha src/*-spec.js test/*-spec.js",
    "commit": "git-issues && commit-wizard",
    "issues": "git-issues",
    "pkgfiles": "pkgfiles",
    "size": "tarball=\"$(npm pack .)\"; wc -c \"${tarball}\"; tar tvf \"${tarball}\"; rm \"${tarball}\";"
  },
  "files": [
    "describe-it.js",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/describe-it.git"
  },
  "keywords": [
    "bdd",
    "testing",
    "unit",
    "testing",
    "mocha",
    "jasmine",
    "spec",
    "helper",
    "require",
    "function"
  ],
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahmutov/describe-it/issues"
  },
  "homepage": "https://github.com/bahmutov/describe-it",
  "dependencies": {
    "check-more-types": "2.2.0",
    "debug": "2.2.0",
    "lazy-ass": "1.1.0",
    "really-need": "1.7.2"
  },
  "devDependencies": {
    "git-issues": "1.2.0",
    "mocha": "2.3.4",
    "pkgfiles": "2.3.0",
    "pre-git": "1.3.1"
  },
  "config": {
    "pre-git": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": [
        "npm test"
      ],
      "pre-push": [
        "npm run size",
        "npm run pkgfiles"
      ],
      "post-commit": [
        "npm version"
      ],
      "post-merge": []
    }
  }
}
